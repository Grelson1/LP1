#include <stdio.h>

struct Equipe{
    int nome;
    int v;
    int d;
};

void preenche (struct Equipe* e){
    printf ("Nome da equipe: ");
    scanf ("%d", (*e).nome);
    (*e).v = 0;
    (*e).d = 0;
}

int main (void){
    struct Equipe equipe1;
    preenche(&equipe1);
    printf("Equipe: %d\n Vitorias= %d\n Derrotas= %d\n", equipe1.nome, equipe1.v, equipe1.d);
    return 0;
}
